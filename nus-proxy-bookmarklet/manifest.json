{
  "name": "NUS Proxy Bookmarklet",
  "version": "0.1.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["main.js"]
  }],
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  },
  "permissions": ["contextMenus"]
}
